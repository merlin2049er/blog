<!DOCTYPE html>
<html>
  <head>
    <title>Blog</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div id='sidebar' >
      <div id='logo'>
         <%= link_to root do %>
          <%= image_tag 'logo.svg' %>
        <% end %>

      </div>
      <ul>
        <li class='category'>Website</li>
        <li> <%= link_to "Blog"</li>
        <li></li>
      </ul>
    </div>

    <%= yield %>
  </body>
</html>
